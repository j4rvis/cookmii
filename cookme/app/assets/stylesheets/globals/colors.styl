$orange = #ffb300
$orange-light = #ffca28
$orange-dark = #ffa000
$green = #12c700
$green-light = #64dd17
$green-dark = #64dd17
$grey = #9e9e9e
$grey-light = #f9f9f9
$grey-dark = #757575

desktop = "screen and (min-device-width: 1000px) and (min-width: 1000px)"
mobile = "screen and (max-device-width: 1000px)"

.is-orange
  color $orange

.is-orange-dark
  color $orange

.is-green
  color $green

.is-green-dark
  color $green-dark
