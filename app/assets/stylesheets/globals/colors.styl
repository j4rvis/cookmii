$orange = #ffb300
$orange-light = #ffca28
$orange-dark = #EF6C00
$green = #43A047
$green-light = #66BB6A
$green-dark = #1B5E20
$grey = #E9E9E9
$grey-light = #f9f9f9
$grey-dark = #BDBDBD

desktop = "screen and (min-device-width: 1000px) and (min-width: 1000px)"
mobile = "screen and (max-device-width: 1000px)"

.is-orange
  color $orange

.is-orange-dark
  color $orange

.is-green
  color $green

.is-green-dark
  color $green-dark
